config wifi-device 'radio0'
  option type 'mac80211'
  option path 'platform/10300000.wmac'
  option htmode 'HT20'
  option country 'DK'

config wifi-iface 'wwan'
  option device 'radio0'
  option mode 'sta'
  option network 'wwan'
  option ssid 'UPSTREAM_SSID'
  option encryption 'psk2'
  option key 'UPSTREAM_PASSWORD'
