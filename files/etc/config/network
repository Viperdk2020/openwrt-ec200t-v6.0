config interface 'loopback'
    option device 'lo'
    option proto 'static'
    option ipaddr '127.0.0.1'
    option netmask '255.0.0.0'

config globals 'globals'
    option ula_prefix 'fd00:7628:ec20::/48'

config device 'br_lan'
    option name 'br-lan'
    option type 'bridge'
    list ports 'eth0'

config interface 'lan'
    option device 'br-lan'
    option proto 'static'
    option ipaddr '192.168.1.1'
    option netmask '255.255.255.0'
    option ip6assign '0'

config interface 'wwan'
    option proto 'dhcp'
    option peerdns '1'
    option ipv6 '0'
    option metric '10'

config interface 'cell'
    option proto 'ppp'
    option device '/dev/ttyUSB3'
    option apn 'YOUR_APN'
    option dialnumber '*99#'
    option ipv6 '0'
    option defaultroute '1'
    option peerdns '1'
    option metric '20'
    option maxfail '0'
    option holdoff '5'

config interface 'wan_usb'
    option proto 'dhcp'
    option device 'usb0'
    option peerdns '1'
    option ipv6 '0'
    option metric '30'
