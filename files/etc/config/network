config interface 'loopback'
        option ifname 'lo'
        option proto 'static'
        option ipaddr '127.0.0.1'
        option netmask '255.0.0.0'

config interface 'lan'
        option ifname 'eth0'
        option proto 'static'
        option ipaddr '192.168.1.1'
        option netmask '255.255.255.0'

config interface 'wwan'
        option proto 'dhcp'
        option metric '10'

config interface 'cell'
        option proto 'ppp'
        option device '/dev/ttyUSB3'
        option apn 'data.lycamobile.dk'
        option username ''
        option password ''
        option metric '20'

config interface 'usb'
        option ifname 'usb0'
        option proto 'dhcp'
        option metric '30'

